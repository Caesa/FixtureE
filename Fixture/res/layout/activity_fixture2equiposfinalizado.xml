<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <TextView
        android:id="@+id/lblCampeon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:text="Caesa FC"
        android:textSize="@dimen/Ganador" />

    <TextView
        android:id="@+id/lblCampeonf4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/lblCampeon"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="21dp"
        android:text="@string/Ganador"
        android:textSize="@dimen/Nombrequipo" />

    <Button
        android:id="@+id/btnFinf2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:onClick="reset"
        android:text="@string/btnVolvermprinc" />

</RelativeLayout>
